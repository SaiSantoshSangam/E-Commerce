{
  "expo": {
    "name": "ShopBy",
    "slug": "ShopBy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.bmp",
    "splash": {
      "image": "./assets/icon.bmp",
      "resizeMode": "center",
      "backgroundColor": "#F4F4F4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.bmp",
        "backgroundColor": "#F4F4F4"
      }
    },
    "web": {
      "favicon": "./assets/WebHomeScreen.jpg"
    }
  }
}
